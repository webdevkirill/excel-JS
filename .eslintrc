{
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "no-trailing-spaces": "off",
        "operator-linebreak": "off",
        "max-len": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"],
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    }
}