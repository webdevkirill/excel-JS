{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "no-trailing-spaces": "off",
        "operator-linebreak": "off",
        "max-len": "off"
    },
    "extends": ["eslint:recommended", "google"]
}